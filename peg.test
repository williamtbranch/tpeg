# Match language of a character 'a'
Grammar a{
    A <- 'a'
}
Test match a {
    a
    >Expect true
    b
    >Expect false
    aa
    >Expect false
}

Test search a {
    a
    >Expect {
        (1 1 1)
    }
    aa
    >Expect {
        (1 1 1) (1 2 1)
    }
    abb
    bba
    cd
    >Expect {
        (1 1 1) (2 3 1)
    }
    xxz
    zxx
    >Expect {}
}
